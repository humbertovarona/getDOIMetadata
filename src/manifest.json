{
    "manifest_version": 3,
    "name": "DOI Metadata Extractor",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "Metadata48.png",
        "128": "Metadata128.png"
      }
    },
    "permissions": [
      "activeTab",
      "scripting",
      "clipboardWrite"
    ],
    "host_permissions": [
      "*://api.crossref.org/*"
    ]
  }
  
